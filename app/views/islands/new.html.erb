  <div class="container">
    <%= simple_form_for [@island] do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :availability %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit "Save Island", class: "btn btn-primary" %>
  <% end %>
  </div>


